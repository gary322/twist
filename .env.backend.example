# Backend (Docker Compose) example environment.
#
# Copy to `.env.backend` on your VM and fill values.

NODE_ENV=production

DB_USERNAME=twist
DB_PASSWORD=change-me
DB_NAME=twist_influencer

REDIS_PASSWORD=change-me

JWT_SECRET=change-me-64-chars
JWT_EXPIRES_IN=15m

# Comma-separated origins for browser clients
ALLOWED_ORIGINS=https://twist.example,https://app.twist.example

# Optional
INFLUENCER_API_PORT=3003
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
STAKING_PROGRAM_ID=
WALLET_PRIVATE_KEY=

